<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/splashFragment">
    <fragment
        android:id="@+id/splashFragment"
        android:name="com.bdtask.restoraposlite.Fragment.SplashFragment"
        android:label="fragment_splash"
        tools:layout="@layout/fragment_splash">
        <action
            android:id="@+id/splashFrag2homeFrag"
            app:destination="@id/homeFragment"
            app:popUpTo="@id/splashFragment"
            app:popUpToInclusive="true"
            app:enterAnim="@anim/enter"
            app:exitAnim="@anim/exit" />
        <action
            android:id="@+id/splashFrag2onboardFrag"
            app:destination="@id/onboardFragment"
            app:popUpTo="@id/splashFragment"
            app:popUpToInclusive="true"
            app:enterAnim="@anim/enter"
            app:exitAnim="@anim/exit" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.bdtask.restoraposlite.Fragment.MainFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_main">
        <action
            android:id="@+id/homeFrag2orderInfoFrag"
            app:destination="@id/orderInfoFragment"
            app:enterAnim="@anim/enter"
            app:exitAnim="@anim/exit"
            app:popEnterAnim="@anim/pop_enter"
            app:popExitAnim="@anim/pop_exit" />
        <action
            android:id="@+id/homeFrag2ongoingFrag"
            app:destination="@id/ongoingFragment"/>
        <action
            android:id="@+id/homeFrag2todayFrag"
            app:destination="@id/todayFragment"/>
        <action
            android:id="@+id/homeFrag2foodFrag"
            app:destination="@id/foodFragment"
            app:enterAnim="@anim/enter"
            app:exitAnim="@anim/exit"
            app:popEnterAnim="@anim/pop_enter"
            app:popExitAnim="@anim/pop_exit"/>
        <action
            android:id="@+id/homeFrag2comFrag"
            app:destination="@id/completedFragment" />
        <action
            android:id="@+id/homeFrag2canFrag"
            app:destination="@id/canceledFragment" />
        <action
            android:id="@+id/homeFrag2aboutUs"
            app:destination="@id/aboutUsFragment"
            app:enterAnim="@anim/enter"
            app:exitAnim="@anim/exit"
            app:popEnterAnim="@anim/pop_enter"
            app:popExitAnim="@anim/pop_exit"/>
        <action
            android:id="@+id/homeFrag2repoFrag"
            app:destination="@id/reportFragment"
            app:enterAnim="@anim/enter"
            app:exitAnim="@anim/exit"
            app:popEnterAnim="@anim/pop_enter"
            app:popExitAnim="@anim/pop_exit"/>
        <action
            android:id="@+id/action_homeFragment_to_settingFragment"
            app:destination="@id/settingFragment" />
        <action
            android:id="@+id/homeFrag2settingFrag"
            app:destination="@id/settingFragment"
            app:enterAnim="@anim/enter"
            app:exitAnim="@anim/exit"
            app:popEnterAnim="@anim/pop_enter"
            app:popExitAnim="@anim/pop_exit"/>
    </fragment>
    <fragment
        android:id="@+id/ongoingFragment"
        android:name="com.bdtask.restoraposlite.Fragment.OngoingFragment"
        android:label="fragment_ongoing"
        tools:layout="@layout/fragment_ongoing">
        <action
            android:id="@+id/ongFrag2homeFrag"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/todayFragment"
        android:name="com.bdtask.restoraposlite.Fragment.TodayFragment"
        android:label="fragment_today"
        tools:layout="@layout/fragment_today">

    </fragment>
    <fragment
        android:id="@+id/orderInfoFragment"
        android:name="com.bdtask.restoraposlite.Fragment.OrderInfoFragment"
        android:label="fragment_order_info"
        tools:layout="@layout/fragment_order_info">

    </fragment>
    <fragment
        android:id="@+id/foodFragment"
        android:name="com.bdtask.restoraposlite.Fragment.FoodFragment"
        android:label="fragment_food"
        tools:layout="@layout/fragment_food">
    </fragment>
    <fragment
        android:id="@+id/completedFragment"
        android:name="com.bdtask.restoraposlite.Fragment.CompletedFragment"
        android:label="CompletedFragment" />
    <fragment
        android:id="@+id/canceledFragment"
        android:name="com.bdtask.restoraposlite.Fragment.CanceledFragment"
        android:label="fragment_canceled"
        tools:layout="@layout/fragment_canceled" />
    <fragment
        android:id="@+id/aboutUsFragment"
        android:name="com.bdtask.restoraposlite.Fragment.AboutUsFragment"
        android:label="fragment_aboutus"
        tools:layout="@layout/fragment_aboutus" />
    <fragment
        android:id="@+id/reportFragment"
        android:name="com.bdtask.restoraposlite.Fragment.ReportFragment"
        android:label="ReportFragment" />
    <fragment
        android:id="@+id/onboardFragment"
        android:name="com.bdtask.restoraposlite.Fragment.OnboardFragment"
        android:label="fragment_onboard"
        tools:layout="@layout/fragment_onboard" >
        <action
            android:id="@+id/onboardFrag2settingFrag"
            app:destination="@id/settingFragment"
            app:popUpTo="@id/onboardFragment"
            app:popUpToInclusive="true"
            app:enterAnim="@anim/enter"
            app:exitAnim="@anim/exit" />
    </fragment>
    <fragment
        android:id="@+id/settingFragment"
        android:name="com.bdtask.restoraposlite.Fragment.SettingFragment"
        android:label="fragment_setting"
        tools:layout="@layout/fragment_setting" >
        <action
            android:id="@+id/settingFrag2homeFrag"
            app:destination="@id/homeFragment"
            app:popUpTo="@id/settingFragment"
            app:popUpToInclusive="true"
            app:enterAnim="@anim/enter"
            app:exitAnim="@anim/exit" />
    </fragment>
</navigation>